package evaluation;

import java.util.HashMap;
import java.util.Map;

public class Level {
	int points;
	private Map<String, Journal> journals = new HashMap<>();

	public Level(int p) {
		this.points = p;
	}

	public int getPoints() {
		return points;
	}

	public void addJournal(Journal j) {
		journals.put(j.getName(), j);
		
	}

}
